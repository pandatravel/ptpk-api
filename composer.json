{
    "name": "ammonkc/ptpkg-api",
    "description": "Ptpkg API v1 Client",
    "version": "0.1.0-dev",
    "type": "library",
    "keywords": ["ptpkg", "panda", "api"],
    "license": "MIT",
    "homepage": "https://github.com/ammonkc/ptpkg-api",
    "authors": [
        {
            "name": "Ammon Casey",
            "email": "ammon@caseyohana.com",
            "role" : "Lead Developer"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/psr7": "^1.2",
        "kamermans/guzzle-oauth2-subscriber": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "symfony/var-dumper": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Ammonkc\\Ptpkg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ammonkc\\Ptpkg\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
